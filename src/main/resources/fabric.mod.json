{
	"schemaVersion": 1,
	"id": "uncraftables",
	"version": "${version}",
	"name": "Uncraftables",
	"description": "Implements crafting recipes for non-craftable vanilla items, with a focus on fitting with the game's progression and reducing the grinding aspect of the game.",
	"authors": [
		"zayphi"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/uncraftables",
		"sources": "https://github.com/zayphi/uncraftables",
		"issues": "https://github.com/zayphi/uncraftables/issues"
	},
	"license": "MIT",
	"icon": "assets/uncraftables/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"io.github.zayphi.Uncraftables"
		]
	},
	"mixins": [
		"uncraftables.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.0",
		"minecraft": ">=1.19.2",
		"java": ">=17",
		"fabric-api": "*"
	}
}